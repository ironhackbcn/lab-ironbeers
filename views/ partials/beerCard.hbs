<div class="beer" style="width: 33.333%;">
  <img class="beer-img-top" src="{{this.image_url}}" alt="{{this.name}}">
  <div class="beer-body">
    <h5 class="beer-title">{{this.name}}</h5>
    <p class="beer-tagline">{{this.tagline}}</p>
    <p class="beer-description">{{this.description}}</p>

  </div>
</div>